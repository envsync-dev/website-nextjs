    "@mui/material-nextjs": "^6.1.6",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.46.1",
    "@toolpad/core": "^0.9.0",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "next": "15.0.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
